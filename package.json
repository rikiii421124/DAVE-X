{
  "name": "peshxdave",
  "version": "3.2.0",
  "description": "Simple nodejs WhatsApp Bot by Dave Tech",
  "main": "index.js",
  "scripts": {
    "start": "node --max-old-space-size=180 --optimize-for-size --gc-interval=50 --expose-gc index.js",
    "start:optimized": "node --max-old-space-size=180 --optimize-for-size --gc-interval=50 --expose-gc index.js",
    "cleanup": "node cleanup.js",
    "reset-session": "node reset-session.js",
    "start:clean": "npm run cleanup && npm run start:optimized",
    "start:fresh": "npm run reset-session && npm start",
    "install:panel": "npm install --legacy-peer-deps",
    "install:force": "npm install --force",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:build": "docker build -t docker run -e SESSION_ID=$SESSION_ID dave-x",
    "dev": "npx tsx server/index.ts"
  },
  "keywords": [
    "whatsapp-bot"
  ],
  "author": "Dave",
  "license": "ISC",
